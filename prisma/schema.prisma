// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Product {
  id          String @id @default(uuid())
  image       String
  description String
  tags        String
  title       String
  priceTo     String
  priceFrom   String
}
